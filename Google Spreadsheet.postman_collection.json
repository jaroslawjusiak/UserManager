{
	"info": {
		"_postman_id": "73997d74-ad3a-45ba-8747-03e8d4823bc4",
		"name": "Google Spreadsheet",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Get CSV from Google Spreadsheet",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "93b8fbdc-c8b6-4c49-a075-dbadf2732cc5",
						"exec": [
							"pm.test.skip(\"Getting CSV file from Google Spreadsheet\", function(){",
							"    console.log(\"https://docs.google.com/spreadsheets/u/0/d/:id/export?format=csv\");",
							"    console.log(\"https://docs.google.com/spreadsheets/u/0/d/1xr9OtQMJv_qu3Zy2qsTk4g8VHjFf3Ok5UbUaveXn2MI/export?format=csv\");",
							"} )",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://docs.google.com/spreadsheets/u/0/d/:id/export?format=csv",
					"protocol": "https",
					"host": [
						"docs",
						"google",
						"com"
					],
					"path": [
						"spreadsheets",
						"u",
						"0",
						"d",
						":id",
						"export"
					],
					"query": [
						{
							"key": "format",
							"value": "csv"
						}
					],
					"variable": [
						{
							"key": "id",
							"value": ""
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Latest For Base Currency",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "56e40803-2883-4973-b7c6-7c9a82ac75a9",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var currency = data.CurrencyCode;",
							"console.log(currency);",
							"",
							"pm.test(\"Base currency should be equal to: \" + currency, function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.base).to.eql(currency);",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://api.exchangeratesapi.io/latest?base={{CurrencyCode}}",
					"protocol": "https",
					"host": [
						"api",
						"exchangeratesapi",
						"io"
					],
					"path": [
						"latest"
					],
					"query": [
						{
							"key": "base",
							"value": "{{CurrencyCode}}"
						}
					]
				}
			},
			"response": []
		}
	],
	"protocolProfileBehavior": {}
}